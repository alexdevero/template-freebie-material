{
  "name": "template-material-30",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "> 4%",
    "Chrome >= 64",
    "Edge > 13",
    "Firefox >= 59",
    "IE 11",
    "last 4 versions",
    "not dead",
    "Safari >= 9"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "copy:bootstrap:css": "cp ./node_modules/bootstrap/dist/css/bootstrap.min.css ./public/styles",
    "copy:bootstrap:js": "cp ./node_modules/bootstrap/dist/js/bootstrap.min.js ./public/scripts",
    "copy:jquery": "cp ./node_modules/jquery/dist/jquery.slim.min.js ./public/scripts",
    "copy:popper": "cp ./node_modules/popper.js/dist/popper.min.js ./public/scripts",
    "copy": "npm run copy:bootstrap:css && npm run copy:bootstrap:js && npm run copy:jquery && npm run copy:popper",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.3"
  }
}
