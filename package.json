{
  "name": "template-material-30",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "copy:bootstrap:css": "cp ./node_modules/bootstrap/dist/css/bootstrap.min.css ./public/styles",
    "copy:bootstrap:js": "cp ./node_modules/bootstrap/dist/js/bootstrap.min.js ./public/scripts",
    "copy:jquery": "cp ./node_modules/jquery/dist/jquery.slim.min.js ./public/scripts",
    "copy:popper": "cp ./node_modules/popper.js/dist/popper.min.js ./public/scripts",
    "copy": "npm run copy:bootstrap:css && npm run copy:bootstrap:js && npm run copy:jquery && npm run copy:popper",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "postcss-assets": "^5.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-responsive-type": "^1.0.0",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.0.2",
    "react-scripts": "2.1.3"
  }
}
